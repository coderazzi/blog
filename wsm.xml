    </location>
    <location source='Java/tablefilter/api' target='tablefilter/api'>
        <template mold='../../../javadoc.mold.htm'>
            <exclude pattern='index.html'/>
            <exclude pattern='*-frame.html'/>
        </template>
    </location>
    <location source='tnotes'>
        <template mold='../mold.htm'/>
    </location>
    <location source='howto'>
        <template mold='../mold.htm'/>
    </location>
    <location source='books'>
        <template mold='../mold.htm'/>
    </location>
    <location source='linux'>
