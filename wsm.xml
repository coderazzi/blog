    </location>       
    <location source='solaris'>
        <template mold='../mold.htm'/>
    </location>       
    <location source='phd'>
        <template mold='../mold.htm'/>
    </location>       
    <location source='css'/>
    <location source='js'/>
    <location source='private'/>
    <location source='java/idldepend' target='idldepend'>
        <template mold='../../special.mold.htm' pattern='index.htm' dismissSourceHead='true'/>
    </location>
    <location source='java/glassedpane' target='glassedpane'>
        <template mold='../../special.mold.htm' pattern='index.htm' dismissSourceHead='true'/>
        <exclude pattern='api'/>
    </location>
    <location source='java/glassedpane/api' target='glassedpane/api'>
        <template mold='../../../javadoc.mold.htm'>
            <exclude pattern='index.html'/>
            <exclude pattern='*-frame.html'/>
        </template>
    </location>
    <location source='csharp/xmlobjects'>
        <template mold='../../special.mold.htm' pattern='index.htm' dismissSourceHead='true'/>
    </location>
    <location source='csharp/xmlobjectsdocgui'>
        <template mold='../../special.mold.htm' pattern='index.htm' dismissSourceHead='true'/>
    </location>
    <location source='java/tablefilter' target='tablefilter'>
        <template mold='../../tfilter.mold.htm' dismissSourceHead='true'/>
        <exclude pattern='api'/>
        <exclude pattern='api5'/>
    </location>
    <location source='java/tablefilter/api' target='tablefilter/api'>
        <template mold='../../../javadoc.mold.htm'>
            <exclude pattern='index.html'/>
            <exclude pattern='*-frame.html'/>
        </template>
    </location>
    <location source='java/tablefilter/api5' target='tablefilter/api5'>
        <template mold='../../../javadoc.mold.htm'>
            <exclude pattern='index.html'/>
            <exclude pattern='*-frame.html'/>
        </template>
    </location>
    <location source='tnotes'>
        <template mold='../mold.htm'/>
    </location>
    <location source='howto'>
        <template mold='../mold.htm'/>
    </location>
    <location source='books'>
        <template mold='../mold.htm'/>
    </location>
    <location source='linux'>
        <template mold='../mold.htm'/>
    </location>
    <exclude pattern='.svn'/>
    <exclude pattern='*.mold.htm'/>
</transform>




