<?xml version="1.0" standalone="yes"?>

<wsm path='website'>
<transform source='.'>
    <alltemplates cssMoldClass='wsmreplace'>
        <replace what='cssdir' withdir='css'/>
        <replace what='wsmdoc' withdoc='full'/>
    </alltemplates>
    <location source='main' target='.'>
        <template mold='../mold.htm'/>
    </location>       
    <location source='java' target='.'>
        <template mold='../mold.htm'/>
    </location>       
    <location source='python'>
        <template mold='../mold.htm'/>
