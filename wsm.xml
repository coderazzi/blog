    </location>       
    <location source='css'/>
    <location source='Java/idldepend' target='idldepend'>
        <template mold='../../special.mold.htm' pattern='index.htm' dismissSourceHead='true'/>
    </location>
    <location source='Java/glassedpane' target='glassedpane'>
        <template mold='../../special.mold.htm' pattern='index.htm' dismissSourceHead='true'/>
        <exclude pattern='api'/>
    </location>
    <location source='Java/glassedpane/api' target='glassedpane/api'>
        <template mold='../../../javadoc.mold.htm'>
            <exclude pattern='index.html'/>
